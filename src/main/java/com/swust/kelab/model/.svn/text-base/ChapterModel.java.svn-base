package com.swust.kelab.model;

import java.util.ArrayList;
import java.util.List;

import com.swust.kelab.domain.Resource;

public class ChapterModel {

	private Integer chapId;
	private String chapName;
	private String courName;
	private Integer resoType;
	private Integer courId;
	private String chapOrdinal;
	private String chapDescribe;
	private String resoVedioTime;
	private List<Resource> resources = new ArrayList<Resource>();
	
	public Integer getChapId() {
		return chapId;
	}
	public void setChapId(Integer chapId) {
		this.chapId = chapId;
	}
	public String getChapName() {
		return chapName;
	}
	public void setChapName(String chapName) {
		this.chapName = chapName;
	}
	public String getCourName() {
		return courName;
	}
	public void setCourName(String courName) {
		this.courName = courName;
	}
	public List<Resource> getResources() {
		return resources;
	}
	public void setResources(List<Resource> resources) {
		this.resources = resources;
	}
	public String getChapOrdinal() {
		return chapOrdinal;
	}
	public void setChapOrdinal(String chapOrdinal) {
		this.chapOrdinal = chapOrdinal;
	}
	public String getChapDescribe() {
		return chapDescribe;
	}
	public void setChapDescribe(String chapDescribe) {
		this.chapDescribe = chapDescribe;
	}
	public Integer getCourId() {
		return courId;
	}
	public void setCourId(Integer courId) {
		this.courId = courId;
	}
	public Integer getResoType() {
		return resoType;
	}
	public void setResoType(Integer resoType) {
		this.resoType = resoType;
	}
	public String getResoVedioTime() {
		return resoVedioTime;
	}
	public void setResoVedioTime(String resoVedioTime) {
		this.resoVedioTime = resoVedioTime;
	}
}
