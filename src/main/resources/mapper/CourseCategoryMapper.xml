<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="courseCategory">
	<select id="all" resultType="com.swust.kelab.domain.CourseCategory">
		<![CDATA[
			SELECT 
				cc.coca_id as cocaId,
				cc.coca_name as cocaName
			FROM CourseCategory cc
	]]>
	</select>
</mapper>