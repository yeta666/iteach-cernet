@CHARSET "UTF-8";
#myExamInfo a {
	display: block;
	float: left;
	padding: 2px;
	border: solid 2px #ddd;
	background: #fff;
	width: 30px;
	height: 30px;
	text-decoration: none;
}

#myExamInfo :hover {
	background: #00CCCC;
}

#examList {
	left: 93%;
	position: fixed;
	margin-top: 200px;
	background: #0000;
	padding: 12px;
	float: right;
	z-index: 9999;
}

#box {
	display: none;
	left: 85%;
	margin-top: 200px;
	position: fixed;
	float: left;
	width: 200px;
	height: auto;
	background: #fff;
	/* border: 1px solid #333; */
	padding: 12px;
	text-align: center;
	z-index: 9999;
}

#myExamInfo a.finished {
	background: #ADD8E6;
}
