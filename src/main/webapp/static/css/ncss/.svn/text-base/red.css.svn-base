.nav>li>a {
	color: #a7b1c2;
	font-weight: 600;
	padding: 14px 20px 14px 25px;
}
.nav .nav-second-level>li>a {
	padding-left:55px;
}

.navbar-default .nav > li > a:hover, .navbar-default .nav > li > a:focus{
	background-color:#fcc;
}
.nav > li.active > a {
    color: #f66;
}
.nav > li > a {
    color: #c69;
}
.nav > li.active {
    background: #fcc none repeat scroll 0 0;
    border-left: 6px solid #F08080;
}
.skin-1.mini-navbar .nav-second-level {
	background: #EEA9B8;
}
.nav-header {
    background: rgba(0, 0, 0, 0) url("../../img/header-profile-red.png") no-repeat scroll 0 0;
    padding: 33px 25px;
}
.navbar-default .nav>li>a:hover,.skin-1 .navbar-default .nav>li>a:focus
	{
	background: #fcc;
	color: #fff;
}
#wrapper {
	background-color: #EEA9B8;
}
.nav-header .text-muted{
	color: #cc6699;
}