.nav>li>a {
	color: #a7b1c2;
	font-weight: 600;
	padding: 14px 20px 14px 25px;
}
.nav .nav-second-level>li>a {
	padding-left:55px;
}

.navbar-default .nav > li > a:hover, .navbar-default .nav > li > a:focus{
	background-color:#162934;
}
.nav > li.active > a {
    color: #fff;
}
.nav > li > a {
    color: #9ea6b9;
}
.nav > li.active {
    background: #162934 none repeat scroll 0 0;
    border-left: 4px solid #19aa8d;
}
.skin-1.mini-navbar .nav-second-level {
	background:#293846;
}
.nav-header {
    background: rgba(0, 0, 0, 0) url("../../img/header-profile.png") no-repeat scroll 0 0;
    padding: 33px 25px;
}
.navbar-default .nav>li>a:hover,.skin-1 .navbar-default .nav>li>a:focus
	{
	background: #162934;
	color: #fff;
}
#wrapper {
	background-color:#2f4050;
}
