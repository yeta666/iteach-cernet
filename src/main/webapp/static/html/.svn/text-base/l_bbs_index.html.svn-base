							<div id="content">
								<!--二级栏目  -->
								
								<div id="myTabContent" class="tab-content" >
									<div id="viewBbslable" >
										<ul class="nav nav-pills" id="bbscourselist"></ul>
										<!--显示课程列表  -->
									</div>
									<!-- 按钮区 -->
									<div>
									<form onsubmit="return false;" class="form-search">
										<a type="button" class="btn btn-info" id="newPost"
											href="bbs_newPost.html?firstCol=3&secondCol=18"><i
											class=" icon-comment icon-white"> </i>发起讨论</a> <a type="button"
											class="btn btn-success" id="myPost"
											onclick="javascript:myDiscss();"> <i
											class=" icon-heart icon-white"></i>我的讨论
										</a> <a type="button" class="btn btn-warning" id="myReply"
											onclick="javascript:myReply();"> <i
											class="icon-edit  icon-white"></i>我的回复
										</a>
									 	<span class="form-search pull-right">
											<input id="searchKeyWord" name="searchKeyd" type="text"
												class="input-medium search-query" placeholder="输入发帖人姓名">
											<button id="searchPost" type="button"
												class="btn btn-success">搜索</button>
										</span> 
									</form>
									</div>
									<div id="viewPostList">
										<!--数据显示  -->
										<table class="table table-hover table-bordered"
											id="viewPostListTable">
											<thead>
												<tr>
													<th>序</th>
													<th>主题</th>
													<th>课程</th>
													<th>作者</th>
													<th>发表时间</th>
													<th>人气/回复</th>
												</tr>
											</thead>
											<tbody>
											</tbody>
										</table>
