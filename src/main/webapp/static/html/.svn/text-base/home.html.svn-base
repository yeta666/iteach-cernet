<!--登录弹出层 s -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog"
	aria-labelledby="myModalLabel">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal"
					aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
				<h4 class="modal-title" id="myModalLabel">登录</h4>
				<p id="loginInfo" class="text-center text-info">欢迎登陆学习平台！</p>
			</div>
			<div class="modal-body">
				<div class="form-horizontal">
					<div class="form-group">
						<label for="inputName3" class="col-sm-2 control-label">用户名</label>
						<div class="col-sm-6">
							<input type="text" class="form-control" id="username"
								placeholder="请输入用户名">
						</div>
					</div>
					<div class="form-group">
						<label for="inputPassword3" class="col-sm-2 control-label">密码</label>
						<div class="col-sm-6">
							<input type="password" class="form-control" id="password"
								placeholder="请输入密码">
						</div>
					</div>
					<div class="form-group">
						<label for="inputPassword3" class="col-sm-2 control-label">验证码</label>
						<div class="col-sm-3">
							<input type="text" class="form-control" id="authcode" 
								placeholder="请输入验证码">
						</div>
						<div class="col-sm-3">
							<img  alt="验证码" onclick="reloadcode()" id="imgcode"
								src="checkcode.jsp">
						</div>
					</div>
				</div>

			</div>
			<div class="modal-footer">
				<a href="forgotPassword_home.html" type="button" class="btn btn-default">忘记密码？</a>
				<button id="loginbut" type="button" class="btn btn-primary">登录</button>
			</div>
		</div>
	</div>
</div>
