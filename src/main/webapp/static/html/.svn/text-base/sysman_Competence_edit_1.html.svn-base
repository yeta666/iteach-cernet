<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<link rel="stylesheet" href="../css/iteach.css" />
<link rel="stylesheet" href="../css/bootstrap.css" />
<title>权限管理</title>
</head>
<body>
	<!-- 导航栏目 -->
	<div id="navibar" class="row-fluid"></div>
	<!-- 主框体 -->
	<div class="container main1">
		<div class="row-fluid">
			<div id="pictures" class="span12 pictures"></div>
		</div>
		<div class="row-fluid">
			<div id="column" class="span2 box"></div>
			<div class="span10 well">
				<!--中心界面 -->
				<div class="box">
					<div id="myTabContent" class="tab-content">
						<div class="tab-pane fade active in" id="geren">
							<div id="content">
								<ul class="nav nav-tabs" id="subCols">
								</ul>
								<div class="tab-content">
									<div class="alert alert-error">
										<a class="close" data-dismiss="alert">×</a>
										<h3 class="alert-heading">角色管理须知：</h3>
										<p>对于角色的分配。</p>
									</div>
									<div id="infomsg"></div>
									<!-- 修改角色对话框 -->
									<form id="roleinfo" class="form-inline well">
										角色名称：<input type="text" name="rolename" id="rolename"
											class="span3" placeholder="请输入角色名称" /> 角色描述：<input
											type="text" name="roledesc" id="roledesc" class="span4"
											placeholder="请输入角色描述" /> <a id="saveRole" type="button"
											class="btn btn-primary">保存</a> <a id="cancel" type="button"
											class="btn btn-warning">返回</a>
									</form>
									<div id="res-center" class="tab-pane fade in active">
										<table id="client"
											class="table table-condensed table-hover table-bordered">
											<thead>
												<tr>
													<th width="100">栏目</th>
													<th width="120">模块</th>
													<th>操作</th>
												</tr>
											</thead>
											<tbody id="mainTable">
												<!-- js生成 -->
											</tbody>
										</table>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div id="footer"> </div>
</body>
<script src="../js/ext/jquery-1.8.3.js"></script>
<script src="../js/extend.js"></script>
<script src="../js/ext/bootstrap.js"></script>
<script type="text/javascript" src="../js/utils.js"></script>
<script type="text/javascript" src="../js/jquery.scrollUp.min.js"></script>
<!-- top1 -->
<script type="text/javascript" src="../js/jquery.unveil.min.js"></script>
<!-- top2 -->
<script type="text/javascript" src="../js/common.js"></script>
<script type="text/javascript" src="../js/cachepage.js"></script>
<script type="text/javascript" src="../js/sysman_Competence_edit.js"></script>
</html>